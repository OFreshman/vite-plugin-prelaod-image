{
  "name": "@vite-plugin-preload-image/monorepo",
  "type": "module",
  "version": "0.2.0",
  "private": true,
  "packageManager": "pnpm@9.14.2",
  "description": "简言之，vite-plugin-preload-image 是一个 Vite 插件，用于在构建时自动预加载页面中的图片资源，提高页面加载性能。",
  "scripts": {
    "prepare": "simple-git-hooks",
    "play": "nr -C playground dev",
    "build": "nr -r --filter=./packages/* build",
    "release": "bumpp -r",
    "lint": "eslint --cache .",
    "lint:fix": "nr lint --fix",
    "lint:type": "tsc --noEmit",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@antfu/ni": "^0.23.1",
    "@antfu/utils": "^0.7.10",
    "@types/node": "^22.9.3",
    "@xiaohe01/eslint-config": "^0.0.7",
    "@xiaohe01/tsconfig": "^0.0.2",
    "bumpp": "^9.8.1",
    "eslint": "^9.15.0",
    "esno": "^4.8.0",
    "lint-staged": "^15.2.10",
    "simple-git-hooks": "^2.11.1",
    "typescript": "~5.6.3",
    "unbuild": "^2.0.0",
    "vite": "^5.4.11",
    "vitest": "^2.1.5"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}